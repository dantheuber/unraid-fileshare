<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>False</Beta>
  <Category>HomeAutomation:</Category>
  <Date>2022-11-10</Date>
  <Changes>
  [b]11.NOV.2022:[/b]Added[br]
  </Changes>
  <Support>http://forums.unraid.net</Support>
  <Name>Simple Fileserver</Name>
  <Description>
  A super basic file server.
  </Description>
  <Registry>http://unraid:5000/simple-fileserver_web/</Registry>
  <GitHub>https://github.com/</GitHub>
  <Repository>localhost:5000/simple-fileserver_web</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>3123</HostPort>
        <ContainerPort>3123</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/simple-fileserver</HostDir>
      <ContainerDir>/server/static</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:3123]</WebUI>
  <Banner>http://i.imgur.com/zXpacAF.png</Banner>
  <Icon>http://i.imgur.com/zXpacAF.png</Icon>
</Container>